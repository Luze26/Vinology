          rdfs:range :VinCuvee .



###  http://www.vin.com/ontologies/vin.owl#hasOrigin

:hasOrigin rdf:type owl:ObjectProperty .



###  http://www.vin.com/ontologies/vin.owl#hasProducteur

:hasProducteur rdf:type owl:ObjectProperty ;
               
               rdfs:domain :Vin ;
               
               rdfs:range :VinProducteur ;
               
               rdfs:subPropertyOf :hasOrigin .



###  http://www.vin.com/ontologies/vin.owl#hasSol

:hasSol rdf:type owl:ObjectProperty ;
        
        rdfs:domain :Vin ;
        
        rdfs:range :VinSol ;
        
        rdfs:subPropertyOf :hasOrigin .



###  http://www.vin.com/ontologies/vin.owl#isCepageOf

:isCepageOf rdf:type owl:ObjectProperty ;
            
            owl:inverseOf :hasCepage .



###  http://www.vin.com/ontologies/vin.owl#isCruOf

:isCruOf rdf:type owl:ObjectProperty ;
         
         owl:inverseOf :hasCru ;
         
         rdfs:subPropertyOf :isOriginOf .



###  http://www.vin.com/ontologies/vin.owl#isCuveeOf

:isCuveeOf rdf:type owl:ObjectProperty ;
           
           owl:inverseOf :hasCuvee .



###  http://www.vin.com/ontologies/vin.owl#isOriginOf

:isOriginOf rdf:type owl:ObjectProperty ;
            
            owl:inverseOf :hasOrigin .



###  http://www.vin.com/ontologies/vin.owl#isProducteur

:isProducteur rdf:type owl:ObjectProperty ;
              
              owl:inverseOf :hasProducteur ;
              
              rdfs:subPropertyOf :isOriginOf .



###  http://www.vin.com/ontologies/vin.owl#isSolOf

:isSolOf rdf:type owl:ObjectProperty ;
         
         owl:inverseOf :hasSol ;
         
         rdfs:subPropertyOf :isOriginOf .





