                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasCepage ;
                       owl:someValuesFrom :VinCepage
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasDegreValue ;
                       owl:someValuesFrom xsd:double
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasCuvee ;
                       owl:someValuesFrom :VinCuvee
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :hasSol ;
                       owl:someValuesFrom :VinSol
                     ] .



